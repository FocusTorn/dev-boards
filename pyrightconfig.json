{
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.venv",
    "**/venv",
    "**/ENV",
    "**/env",
    "**/.uv",
    "**/build",
    "**/dist",
    "**/.eggs",
    "**/*.egg-info",
    "esp-idf",
    "Arduino",
    "__removed",
    "_libs",
    "_projects",
    "___shared",
    "**/.git",
    "**/.vscode",
    "**/.idea"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic"
}

